mode "resize" {
  bindsym Down resize grow height 10 px or 10 ppt
  bindsym Escape mode default
  bindsym Left resize shrink width 10 px or 10 ppt
  bindsym Return mode default
  bindsym Right resize grow width 10 px or 10 ppt
  bindsym Up resize shrink height 10 px or 10 ppt
  bindsym h resize shrink width 10 px or 10 ppt
  bindsym j resize grow height 10 px or 10 ppt
  bindsym k resize shrink height 10 px or 10 ppt
  bindsym l resize grow width 10 px or 10 ppt
}

mode "scratchpad" {
  bindsym Escape mode "default"
  bindsym Return mode "default"
  bindsym m exec swaymsg "[app_id=monitorr] scratchpad show " || exec /nix/store/z5yyd8gnx0as8qzknr7683mrmhff99i2-kitty-0.25.2/bin/kitty --class monitorr -e htop, mode default
  bindsym s exec swaymsg "[app_id=spotiff] scratchpad show " || exec /nix/store/z5yyd8gnx0as8qzknr7683mrmhff99i2-kitty-0.25.2/bin/kitty --class spotiff -e ncspot, mode default
  bindsym t exec swaymsg "[app_id=terrm] scratchpad show " || exec /nix/store/z5yyd8gnx0as8qzknr7683mrmhff99i2-kitty-0.25.2/bin/kitty --class terrm, mode default
  bindsym v exec swaymsg "[app_id=mixerr] scratchpad show " || exec /nix/store/z5yyd8gnx0as8qzknr7683mrmhff99i2-kitty-0.25.2/bin/kitty --class mixerr -e pulsemixer, mode default
}

mode "workspace" {
  bindsym Escape mode "default"
  bindsym Return mode "default"
  bindsym a exec bash ~/.nixos-config/config/sway/add_workspace, mode default
  bindsym r exec bash ~/.nixos-config/config/sway/rename_workspace, mode default
  bindsym s exec bash ~/.nixos-config/config/sway/switch_workspace, mode default
}

